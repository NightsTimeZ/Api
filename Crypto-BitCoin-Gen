Dont Bully My "How To Print"

for v in string.char, 0, 0 do str2 = v break end  -- create void string
for v in string.char, 34, 72 do str = v break end -- put string
for v in string.char, 101, 108 do str = str..v break end -- put string
for v in string.char, 108, 111 do str = str..v break end -- put string
for v in string.char, 32, 119 do str = str..v break end -- put string
for v in string.char, 111, 114 do str = str..v break end -- put string
for v in string.char, 108, 100 do str = str..v break end -- put string
for v in string.char, 32, 105 do str = str..v break end -- put string
for v in string.char, 116, 39 do str = str..v break end -- put string
for v in string.char, 115, 32 do str = str..v break end -- put string
for v in string.char, 112, 114 do str = str..v break end -- put string
for v in string.char, 105, 110 do str = str..v break end -- put string
for v in string.char, 116, 34 do str = str..v break end -- put string
for v in string.sub, str2, 3 do str2 = v break end -- for get real void string 
for _ in print, str, str2 do break end -- for print str = real string str2 = void
